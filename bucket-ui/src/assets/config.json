{
    "name": "Bucket",
    "version": "0.0.1",
    "environment": "development",
    "basehref": "bucket",
    "apiURL": "https://dwd.tudelft.nl/bucket/api",
    "grafanaURL": "https://dwd.tudelft.nl/grafana",
    "authCodeFlow": {
        "issuer": "https://dwd.tudelft.nl:443/",
        "redirectUri": "https://dwd.tudelft.nl:443/bucket/things",
        "clientId": "clients:bucket-ui",
        "responseType": "code",
        "scope": "openid offline profile email dcd:public dcd:things dcd:properties dcd:types dcd:consents",
        "useSilentRefresh": false,
        "showDebugInformation": true,
        "sessionChecksEnabled": false,
        "timeoutFactor": 0.01,
        "clearHashAfterLogin": false
      }
}