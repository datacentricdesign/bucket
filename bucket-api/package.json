  },
  "homepage": "https://github.com/datacentricdesign/bucket#readme",
  "dependencies": {
    "@datacentricdesign/types": "0.0.4",
    "aedes": "0.42.5",
    "apidoc": "0.24.0",
    "class-validator": "0.12.2",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "envalid": "6.0.2",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "helmet": "3.23.3",
    "influx": "5.6.2",
    "jsonwebtoken": "8.5.1",
